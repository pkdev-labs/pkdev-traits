{
    "name": "pkdev/traits",
    "description": "Traits developed by PkDev for use in all projects",
    "version": "1.0.0",
    "type": "library",
    "autoload": {
        "psr-4": {
            "PkDev\\Traits\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "license": "MIT",
    "autoload-dev": {
        "psr-4": {
            "PkDev\\Traits\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit tests",
        "test-coverage": "vendor/bin/phpunit tests --coverage-html coverage"
    },
    "keywords": ["traits", "php"],
    "minimum-stability": "stable",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/WyldHalfling/PkDevTraits"
        }
    ],
    "support": {
        "issues": "https://github.com/WyldHalfling/PkDevTraits/issues",
        "source": "https://github.com/WyldHalfling/PkDevTraits"
    }
}